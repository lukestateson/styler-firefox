{
	"name": "Styler Pro",
	"version": "5.0.2",
	"manifest_version": 2,
	"description": "Add custom CSS styles, JavaScript & jQuery to any websites",
	"browser_action": {
		"default_icon": "32.png",
		"default_popup": "popup.html"
	},
	"background": {
		"page": "background.html"
	},
	"permissions": [
		"tabs",
		"activeTab",
		"http://*/*",
		"https://*/*",
		"storage"
	],
	"icons": { "16": "16.png", "32": "32.png", "48": "48.png", "128": "128.png" }
}